{
  "comments": {
    "lineComment": ";"
  },
  "brackets": [
    ["(", ")"],
    ["[", "]"]
  ],
  "autoClosingPairs": [
    ["(", ")"],
    ["[", "]"],
    ["\"", "\""]
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["[", "]"],
    ["\"", "\""]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*\\(",
      "end": "^\\s*\\)"
    }
  },
  "wordPattern": "[\\w\\+\\-\\*\\/<>=!?:]+",
  "indentationRules": {
    "increaseIndentPattern": "^\\s*\\((?!.*\\)).*$",
    "decreaseIndentPattern": "^\\s*\\)"
  }
}